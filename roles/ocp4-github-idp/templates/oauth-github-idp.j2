apiVersion: config.openshift.io/v1
kind: OAuth
metadata:
  annotations:
    release.openshift.io/create-only: "true"
  name: cluster
spec:
  identityProviders:
  - github:
      clientID: {{ github_client_id }}
      clientSecret:
        name: github-client-secret
      teams: 
        {% for team in github_teams %}
        - {{ team }}
        {% endfor %}

    mappingMethod: claim
    name: githubidp
    type: GitHub





